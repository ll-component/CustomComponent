<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    tools:context=".MainActivity">

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/emptyView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="EmptyView"
        android:gravity="center"
        android:textColor="#ff4b73"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:onClick="onClick"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/formView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="FormView"
        android:gravity="center"
        android:textColor="#ff4b73"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:onClick="onClick"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/shapeTextView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="ShapeTextView"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:onClick="onClick"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/rotateTextView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="RotateTextView"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:onClick="onClick"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/clearEditTextView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="ClearEditTextView"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:onClick="onClick" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/chameleonView"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="ChameleonView"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:onClick="onClick" />

    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/suctionBottomLayout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="SuctionBottomLayout"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:onClick="onClick" />


    <com.ll.uicomponent.ShapeTextView
        android:id="@+id/touchEventTest"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="#ff4b73"
        android:text="TouchEventTest"
        android:gravity="center"
        app:shapeStrokeWidth="0.5dp"
        app:shapeStrokeColor="#ff4b73"
        app:shapeRadius="60dp"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:onClick="onClick" />



</LinearLayout>